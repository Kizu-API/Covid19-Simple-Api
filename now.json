    },
    {
      "src": "/api/covid-badung",
      "dest": "/lambdas/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/covid-buleleng",
      "dest": "/lambdas/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/covid-tabanan",
      "dest": "/lambdas/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/covid-gianyar",
      "dest": "/lambdas/index.js",
      "methods": ["GET"]
    }
  ]
}
